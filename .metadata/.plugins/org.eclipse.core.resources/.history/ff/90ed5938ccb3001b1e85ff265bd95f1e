    @Override
    public TitleDto toTitleDto(TitleEntity title) {
        if ( title == null ) {
            return null;
        }

        TitleDto titleDto = new TitleDto();

        titleDto.setId( title.getId() );
        titleDto.setTitleName( title.getTitleName() );

